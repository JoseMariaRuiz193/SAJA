    <TextView
        android:id="@+id/pyplnom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/monbol"
        android:text="nom"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/applenom" />

    <TextView
        android:id="@+id/pyplticker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="10dp"
        android:fontFamily="@font/monbol"
        android:text="ticker"
        android:textColor="#AEAEAE"
        app:layout_constraintEnd_toStartOf="@+id/pyplprice"
        app:layout_constraintStart_toEndOf="@+id/teslanom"
        app:layout_constraintTop_toBottomOf="@+id/appleticker" />

    <TextView
        android:id="@+id/pyplprice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/monbol"
        android:text="price"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appleprice" />

    <TextView
        android:id="@+id/pyplperneg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/monbol"
        android:text="prueba"
        android:textColor="#f1242c"
        app:layout_constraintStart_toEndOf="@+id/appleprice"
        app:layout_constraintTop_toBottomOf="@+id/appleperneg" />

    <TextView
        android:id="@+id/pyplperpos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/monbol"
        android:text="prueba"
        android:textColor="#5ED5A8"
        app:layout_constraintStart_toEndOf="@+id/appleprice"
        app:layout_constraintTop_toBottomOf="@+id/appleperneg" />

    <TextView
        android:id="@+id/amznnom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/monbol"
        android:text="nom"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pyplnom" />

    <TextView
        android:id="@+id/amznticker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
