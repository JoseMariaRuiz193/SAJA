    <TextView
        android:id="@+id/amznperpos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/monbol"
        android:text="prueba"
        android:textColor="#5ED5A8"
        app:layout_constraintStart_toEndOf="@+id/appleprice"
        app:layout_constraintTop_toBottomOf="@+id/pyplperneg" />

    <TextView
        android:id="@+id/msftnom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/monbol"
        android:text="nom"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/amznnom" />

    <TextView
        android:id="@+id/msftticker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="10dp"
        android:fontFamily="@font/monbol"
        android:text="ticker"
        android:textColor="#AEAEAE"
        app:layout_constraintEnd_toStartOf="@+id/pyplprice"
