<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="#1B232A">

    <Button
        android:id="@+id/buttonlogout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="650dp"
        android:text="LOG OUT"
        android:fontFamily = "@font/monreg"
        android:textColor="#171D22"
        app:backgroundTint="#5ED5A8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/Sombreado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="532dp"
        android:fontFamily="@font/monreg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/fondouser" />

    <TextView
        android:id="@+id/TextBuscador"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="164dp"
        android:layout_marginBottom="140dp"
        android:fontFamily="@font/monreg"
        android:text="Busca una acción"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/encacciones"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="70dp"
        android:fontFamily="@font/monbol"
        android:text="PRINCIPALES ACCIONES"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/teslaprice"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Sombreado" />

    <TextView
        android:id="@+id/nombreempresas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginBottom="35dp"
        android:fontFamily="@font/monbol"
        android:text="Nombre / Ticker"
        android:textColor="#AEAEAE"
        app:layout_constraintBottom_toTopOf="@+id/teslaprice"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/encprecio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginBottom="35dp"
        android:fontFamily="@font/monbol"
        android:text="Precio"
        android:textColor="#AEAEAE"
        app:layout_constraintBottom_toTopOf="@+id/teslaprice"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/cambiodia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="256dp"
        android:layout_marginBottom="35dp"
        android:fontFamily="@font/monbol"
        android:text="%Cambio día"
        android:textColor="#AEAEAE"
        app:layout_constraintBottom_toTopOf="@+id/teslaprice"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/divider"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="7dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintTop_toBottomOf="@+id/nombreempresas"
        tools:layout_editor_absoluteX="-28dp" />

    <TextView
        android:id="@+id/teslaprice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginTop="290dp"
        android:fontFamily="@font/monbol"
        android:text="price"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/teslanom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="85dp"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/monbol"
        android:text="nom"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider" />

    <TextView
        android:id="@+id/teslaticker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="85dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/monbol"
        android:text="ticker"
        android:textColor="#AEAEAE"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/teslanom" />

    <TextView
        android:id="@+id/tslaperpos"
        android:layout_width="wrap_content"
