        android:layout_marginStart="85dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/monbol"
        android:text="nom"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/teslaticker" />

    <TextView
        android:id="@+id/appleticker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="85dp"
        android:layout_marginTop="2dp"
        android:fontFamily="@font/monbol"
        android:text="ticker"
        android:textColor="#AEAEAE"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/applenom" />

    <TextView
        android:id="@+id/appleprice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/monbol"
        android:text="price"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/teslaprice" />

    <TextView
        android:id="@+id/appleperneg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="21dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/monbol"
        android:text="prueba"
        android:textColor="#f1242c"
        app:layout_constraintStart_toEndOf="@+id/appleprice"
