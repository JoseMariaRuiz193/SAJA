        android:fontFamily="@font/monbol"
        android:text="ticker"
        android:textColor="#AEAEAE"
        app:layout_constraintEnd_toStartOf="@+id/pyplprice"
        app:layout_constraintStart_toEndOf="@+id/teslanom"
        app:layout_constraintTop_toBottomOf="@+id/pyplticker" />

    <TextView
        android:id="@+id/amznprice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/monbol"
        android:text="price"
        android:textColor="#FFFFFF"
